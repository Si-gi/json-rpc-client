{
  "name": "si-gi/jsonrpc-php",
  "description": "Client & Server JSON-RPC 2.0 compatible pour PHP 8.4",
  "type": "library",
  "require": {
    "php": "^8.3",
    "ext-curl": "*",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1",
    "phpspec/prophecy": "~1.0",
    "phpspec/prophecy-phpunit": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "JsonRpc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JsonRpc\\Tests\\": "tests/"
    }
  }
}
